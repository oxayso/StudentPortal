
@{
    ViewBag.Title = "UploadRequest";
    Layout = "~/Views/Shared/_LayoutTeacher.cshtml";
}





<h2>Upload a File</h2>

@using (Html.BeginForm("Upload", "Assignments", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    <input type="file" name="file" id="file" /><br />
    <input type="submit" value="Upload"/>
}